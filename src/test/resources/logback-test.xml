<?xml version="1.0" encoding="UTF-8"?>

<configuration>

  <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>%d{yyyy-MM-dd HH:mm:ss} [%thread] %-5level %logger{36} - %msg%n</pattern>
    </encoder>
  </appender>

  <root level="debug">
    <appender-ref ref="STDOUT"/>
  </root>


  
 <logger name="com.shootoff.camera.autocalibration.AutoCalibrationManager" level="TRACE"/>
 <logger name="com.shootoff.camera.perspective.PerspectiveManager" level="TRACE"/>
 <logger name="com.shootoff.camera.DeduplicationProcessor" level="TRACE"/>
 <logger name="com.shootoff.camera.ShotDetector" level="TRACE"/>
 
  
</configuration>